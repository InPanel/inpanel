<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
    <dict>
        <key>Label</key>
        <string>org.inpanel.service.daemon</string>
        <key>ProgramArguments</key>
        <array>
            <string>/usr/local/inpanel/inpanel</string>
        </array>
        <key>StandardOutPath</key>
        <string>/usr/local/inpanel/inpanel/data/logfile.log</string>
        <key>StandardErrorPath</key>
        <string>/usr/local/inpanel/inpanel/data/logfile_error.log</string>
        <key>RunAtLoad</key>
        <true/>
        <key>KeepAlive</key>
        <true/>
    </dict>
</plist>